{
    "name": "gravity",
    "displayName": "Gravity",
    "description": "Gravity language syntax support for Visual Studio Code.",
    "repository": "https://github.com/Dohxis/vscode-gravity",
    "version": "0.0.4",
    "publisher": "Dohxis",
    "icon": "gravity.png",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "gravity",
            "aliases": ["Gravity", "gravity"],
            "extensions": [".gravity"],
            "configuration": "./gravity.configuration.json"
        }],
        "grammars": [{
            "language": "gravity",
            "scopeName": "source.gravity",
            "path": "./syntaxes/gravity.tmLanguage"
        }],
        "snippets": [{
            "language": "gravity",
            "path": "./snippets/gravity.json"
        }]
    }
}
